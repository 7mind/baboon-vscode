{
  "name": "baboon-vscode",
  "displayName": "Baboon DML",
  "description": "Syntax highlighting for Baboon DML",
  "version": "0.0.4",
  "engines": {
    "vscode": "^1.95.0"
  },
  "devDependencies": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "baboon",
        "aliases": [
          "Baboon",
          "baboon"
        ],
        "extensions": [
          ".baboon",
          ".bmo"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "baboon",
        "scopeName": "source.baboon",
        "path": "./syntaxes/baboon.tmLanguage.json"
      }
    ]
  },
  "repository": "https://github.com/7mind/baboon-vscode",
  "publisher": "SeptimalMind",
  "icon": "baboon.png"
}
